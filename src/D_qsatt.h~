  
/***************************************************************************
                 module classe D_qsatt.h  -  description

SALIDA : qsatt : An estimation of the satturated humidity at the surface

                             -------------------
 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL temp_sol, YREAL pb)
  {	
	if(Yt > 1)
	{
		YS1 = qsatcalc(temp_sol, pb);
	}
	else
		YS1 = 0;

	//printf("Yt %d tempsol %e  pb %e  YS1 %e \n",Yt,temp_sol,pb,YS1);

  }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL temp_sol, YREAL pb)
  {
	if(Yt > 1)
	{
		YJ1I1= d_qsatcalc_temp(temp_sol, pb);
		YJ1I2= d_qsatcalc_pres_in(temp_sol, pb);
	}
	else
	{
		YJ1I1=0;
		YJ1I2= 0;
	}
	
	
  }

  //===========================================================================

//************************** FIN DU MODULE D_qsatt.h ********************************














