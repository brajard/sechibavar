  
/***************************************************************************
                 module classe I_comb.h  -  description



SALIDA : temp
                             -------------------
 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL zlev, YREAL z0, YREAL roughheight)
  {
	YS1 = (zlev + roughheight) / z0;
	
  }
	

  //===========================================================================
  //                         methodes  backward
 





  backward (YREAL zlev, YREAL z0, YREAL roughheight)
  {	
	YJ1I1 = YJ1I3= 1/z0;
	YJ1I2 = -(zlev + roughheight) / (z0*z0);
	
  }



  //===========================================================================

//************************** FIN DU MODULE I_comb.h ********************************








