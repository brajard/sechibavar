  
/***************************************************************************
                 module classe H_snow_i.h  -  description
                             -------------------

Description: calcul initial de la quantite de neige

output: YS1 = snow

 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL snow1, YREAL totfrac_nobio, YREAL precip_snow)
  {

	YS1 = snow1 + (1 - totfrac_nobio)*precip_snow;
	//if(YS1 !=0)
	//	printf("paseeee YS1 %e\n",YS1);
		
  }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL snow1, YREAL totfrac_nobio, YREAL precip_snow)
  {
	YJ1I1 = 1;
	YJ1I2 = -precip_snow;
	YJ1I3 = (1 - totfrac_nobio);	
	
  }

  //===========================================================================

//************************** FIN DU MODULE H_snow_i.h ********************************














