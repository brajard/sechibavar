  
/***************************************************************************
                 module classe E_fluxlat.h  -  description

 output                            -------------------
	YS1 = fluxlat

 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL fluxlat_init, YREAL fluxlat)
  {
	if(Yt == 1)
	{
		YS1 = fluxlat_init * flat;

		//printf("fluxlat: %e \n",fluxlat_init);
	}
	else
	{
		YS1 = fluxlat;
	}

	//printf("%d LE %e\n",Yt,YS1);
	

  }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL fluxlat_init, YREAL fluxlat)
  {
	if(Yt == 1)
	{
		YJ1I1 = flat;
		YJ1I2 = 0;
	}
	else
	{
		YJ1I1 = 0;
		YJ1I2 = 1;
	}
	
  }

  //===========================================================================

//************************** FIN DU MODULE E_fluxlat.h ********************************














