  
/***************************************************************************
                 module classe E_transpir.h  -  description

 output                            -------------------
	YS1 = transpir

 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL transpir_init, YREAL transpir)
  {
	if(Yt == 1)
	{
		YS1 = transpir_init * transp;
		//printf("transpir : %e \n",transpir);
	}
	else
	{
		YS1 = transpir;
	}

  }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL transpir_init, YREAL transpir)
  {
	if(Yt == 1)
	{
		YJ1I1 = transp;
		YJ1I2 = 0;
	}
	else
	{
		YJ1I1 = 0;
		YJ1I2 = 1;
	}
	
  }

  //===========================================================================

//************************** FIN DU MODULE E_transpir.h ********************************














