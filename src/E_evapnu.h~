  
/***************************************************************************
                 module classe E_evapnu.h  -  description

 output                            -------------------
	YS1 = evapnu

 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL evapnu_init, YREAL evapnu)
  {
	if(Yt == 1)
	{
		YS1 = evapnu_init * evap;
	}
	else
	{
		YS1 = evapnu;
	}


	//printf("%d   evapnu %e\n",Yt,YS1);

  }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL evapnu_init, YREAL evapnu)
  {
	if(Yt == 1)
	{
		YJ1I1 = evap;
		YJ1I2 = 0;
	}
	else
	{
		YJ1I1 = 0;
		YJ1I2 = 1;
	}
	
  }

  //===========================================================================

//************************** FIN DU MODULE E_evapnu.h ********************************














