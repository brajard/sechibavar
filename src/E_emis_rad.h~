  
/***************************************************************************
                 module classe E_emis_rad.h  -  description
                             -------------------
Energie totale emise 

output---> Rad


 ***************************************************************************/

  //===========================================================================
  //                          methodes forward
  forward (YREAL kemis, YREAL emis_s, YREAL lwdown)
  {
	//if(Yt > 1)
	//{
		YS1=  kemis * emis_s + (1-kemis) * lwdown;
		//printf("      emis_s %e     YS1 %e\n",emis_s,YS1);
	//}



   }
	

  //===========================================================================
  //                         methodes  backward
  backward (YREAL kemis, YREAL emis_s, YREAL lwdown)
  {
	//if(Yt > 1)
	//{
		YJ1I1 = emis_s -lwdown ;
		YJ1I2 = kemis;
		YJ1I3 = (1-kemis);

	//}
	

  }

  //===========================================================================

//************************** FIN DU MODULE E_emis_rad.h ********************************


