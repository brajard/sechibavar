  
/***************************************************************************
                 module classe D_pre_aero.h  -  description

                             -------------------
 ***************************************************************************/

  //===========================================================================
  //                          methodes forward

  forward (YREAL z0, YREAL rug)
  {
	if(z0>0)		
		YS1 = (rug+zlev)/z0;   //YS1 = z0/(rug+zlev);
	else
	{
		YS1 =(rug+zlev)/z0_bare;  //YS1 = z0_bare/(rug+zlev);
		printf("\n\n*******************\nz0 is zero!!\n******************\n\n");
	}

	//printf("%d    z0 %e   rug %e   YS1 %e\n",Yt,z0,rug,YS1);
  }
	

  //===========================================================================
  //                         methodes  backward
 
backward (YREAL z0, YREAL rug)
  {
	if(z0!=0)
	{
		//YJ1I1 = 1/(rug+zlev);
		//YJ1I2 = -z0/pow(rug+zlev,2);
		YJ1I1 = -(rug+zlev)/(z0*z0);
		YJ1I2 = 1/z0;
	}
	else
	{
		//YJ1I1 = 0;
		//YJ1I2 = -1/pow(rug+zlev,2);
		YJ1I1 = 0;
		YJ1I2 = 1/z0_bare;
		printf("\n\n*******************\nz0 is zero!!\n******************\n\n");
	}

	
	
  }



  //===========================================================================

//************************** FIN DU MODULE D_pre_aero.h ********************************








